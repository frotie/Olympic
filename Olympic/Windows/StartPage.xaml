<UserControl x:Class="Olympic.Windows.StartPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Olympic.Windows"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <StackPanel>
            <Button Content="Поиск по автомобилям" Margin="10"
                    Padding="0 20"
                    Click="Button_Click"/>
            <Button Content="Поиск по клиентам" Margin="10"
                    Padding="0 20" Click="Button_Click_1"/>
            <Button Content="Бронирование автомобиля" Margin="10"
                    Padding="0 20" Click="Button_Click_2"/>
        </StackPanel>
    </Grid>
</UserControl>
